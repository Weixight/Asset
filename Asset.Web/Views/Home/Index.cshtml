@{
    ViewData["Title"] = "Home Page";
}
<script src="~/assets/js/jquery-3.2.1.min.js"></script>

<div class="content container-fluid">
    <div class="row">
        <div class="col-md-6 col-sm-6 col-lg-4">
            <div class="dash-widget clearfix card-box bg-primary">
                <span class="dash-widget-icon"><i class="fa fa-handshake-o" aria-hidden="true"></i></span>
                <div class="dash-widget-info">
                    <h3>@ViewBag.Corps</h3>
                    <span>Cooperatives</span>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-lg-4">
            <div class="dash-widget clearfix card-box bg-success">
                <span class="dash-widget-icon"><i class="fa fa-money" aria-hidden="true"></i></span>
                <div class="dash-widget-info">
                    <h5>&#x20A6;@ViewBag.Asset.ToString("##,###.##")</h5>
                    <span>Assets</span>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-lg-4">
            <div class="dash-widget clearfix card-box bg-danger">
                <span class="dash-widget-icon"><i class="fa fa-money"></i></span>
                <div class="dash-widget-info">
                    <h5>&#x20A6;@ViewBag.Liab.ToString("##,###.##")</h5>
                    <span>Liabiulities</span>
                </div>
            </div>
        </div>
        
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-sm-6 text-center">
                    <div class="card-box">
                        <div id="area-chart"></div>
                    </div>
                </div>
                <div class="col-sm-6 text-center">
                    <div class="card-box">
                        <div id="line-chart"></div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-12 text-center">
                    <div class="card-box">
                        <div id="bar-chart"></div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-12 text-center">
                    <div class="card-box">
                        <div id="stacked"></div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-12 text-center">
                    <div class="card-box">
                        <div id="pie-chart"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
